<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yedam.app.location.mapper.LocationMapper">
	
	<!-- 전체조회 -->
	<select id="selectLocationAll" resultType="LocationVO">
		SELECT location_id
			   , street_address
			   , postal_code
			   , city
			   , state_province
			   , country_id
		FROM locations
	</select>

</mapper>